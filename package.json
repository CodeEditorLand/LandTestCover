		"istanbul-lib-instrument": "6.0.1",
		"istanbul-lib-report": "3.0.1",
		"istanbul-lib-source-maps": "4.0.1",
		"istanbul-reports": "3.1.6",
		"mocha": "10.2.0"
	},
	"devDependencies": {
		"@types/istanbul-lib-coverage": "2.0.5",
		"@types/istanbul-lib-hook": "2.0.3",
		"@types/istanbul-lib-instrument": "1.7.6",
		"@types/istanbul-lib-report": "3.0.2",
		"@types/istanbul-lib-source-maps": "4.0.3",
		"@types/istanbul-reports": "3.0.3",
		"@types/mocha": "10.0.3",
		"@types/node": "20.8.9",
		"del": "7.1.0",
		"tslint": "6.1.3",
		"tslint-microsoft-contrib": "6.2.0",
		"typescript": "5.2.2",
		"typescript-esbuild": "0.3.1"
	},
	"publishConfig": {
		"access": "public"
	}
